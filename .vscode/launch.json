    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\integrationBot\\dist\\main.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "preLaunchTask": "tsc: build - integrationBot/tsconfig.json",
            "envFile": "${workspaceFolder}\\integrationBot\\.env"
        }
    ]
}